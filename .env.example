# Airflow Environment Variables
# INSTRUCTIONS: Copy this file to .env and replace the values with your own

# User ID for Airflow containers (Linux)
# Run 'id -u' to get your user ID
AIRFLOW_UID=1000

# Airflow Fernet Key (used to encrypt connection passwords in the database)
# Generate a new key with: python -c "from cryptography.fernet import Fernet; print(Fernet.generate_key().decode())"
AIRFLOW__CORE__FERNET_KEY=your_fernet_key_here

# PostgreSQL Database Credentials
POSTGRES_USER=airflow
POSTGRES_PASSWORD=change_this_password
POSTGRES_DB=airflow

# PgAdmin Credentials
PGADMIN_DEFAULT_EMAIL=admin@example.com
PGADMIN_DEFAULT_PASSWORD=change_this_password

# Airflow Web UI Admin Credentials
_AIRFLOW_WWW_USER_USERNAME=airflow
_AIRFLOW_WWW_USER_PASSWORD=change_this_password
